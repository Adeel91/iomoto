{
  "name": "iomoto-app",
  "version": "1.0.0",
  "description": "Iomoto App based on React/Redux/Redux-Saga/Webpack/Babel.",
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "webpack"
  ],
  "license": "MIT",
  "main": "index.js",
  "homepage": "./",
  "scripts": {
    "hello": "webpack-dev-server",
    "serve": "webpack-dev-server --config webpack.dev.config.js",
    "build:production": "webpack --config webpack.prod.config.js",
    "cypress": "cypress open",
    "cypress:chrome": "cypress run --browser chrome",
    "cypress:edge": "cypress run --browser edge",
    "cypress:firefox": "cypress run --browser firefox",
    "cypress:ci": "start-server-and-test serve http://localhost:8080 cy:run",
    "cy:run": "cypress run --spec 'cypress/integration/*.js'",
    "prettier": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Muhammad Adeel",
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@mui/utils": "^5.4.4",
    "axios": "^0.26.1",
    "i18next": "^21.8.1",
    "i18next-browser-languagedetector": "^6.1.4",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.9",
    "react-lazyload": "^3.2.0",
    "react-loading-skeleton": "^3.1.0",
    "react-overlay-loader": "^0.0.3",
    "react-paginate": "^8.1.3",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "react-router-redux": "^4.0.8",
    "react-toastify": "^8.2.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "simplebar-react": "^2.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@faker-js/faker": "^6.0.0-beta.0",
    "@iconify/react": "^3.1.4",
    "@testing-library/react": "^12.1.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.4",
    "cypress": "^9.5.1",
    "dotenv-webpack": "^7.1.0",
    "eslint-config-prettier": "^8.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^12.3.5",
    "mini-css-extract-plugin": "^2.6.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "react-hot-loader": "^4.13.0",
    "react-simple-animate": "^3.3.12",
    "react-test-renderer": "^17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "sass-loader": "^12.6.0",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "web-vitals": "^2.1.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0",
    "yarn-upgrade-all": "^0.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "prettier --check ."
    }
  },
  "lint-staged": {
    "**/*": [
      "prettier --write .",
      "eslint 'src/**' --fix"
    ]
  }
}
